{
    "name": "fajarmf10/tanggalan",
    "description": "PHP library for Javanese and Hijri calendar conversions with Laravel integration",
    "keywords": ["calendar", "hijri", "javanese", "weton", "pasaran", "indonesia", "laravel", "islamic-calendar"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fajar Maulana Firdaus",
            "email": "fajarmf78@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "orchestra/testbench": "^8.0|^9.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Tanggalan\\": "src/",
            "Tanggalan\\Laravel\\": "laravel/"
        },
        "files": [
            "laravel/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tanggalan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanggalan\\Laravel\\TanggalanServiceProvider"
            ],
            "aliases": {
                "Tanggalan": "Tanggalan\\Laravel\\Facades\\Tanggalan"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
